datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Benefit {
  id     Int     @id @default(autoincrement())
  name   String
}

model Employee {
  id              Int     @id @default(autoincrement())
  firstName       String
  lastName        String
  date_of_birth   DateTime
  secret          String
}
